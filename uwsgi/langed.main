[uwsgi]
socket=0.0.0.0:9100
processes=25
harakiri=300
chdir=/app
module=langed.wsgi:application
master=True
pidfile=/tmp/langed.pid
vacuum=True
max-requests=500
py-autoreload=3
env=DJANGO_SETTINGS_MODULE=langed.settings
env=LANG=ru_RU.utf8
env=LC_ALL=ru_RU.UTF-8
env=LC_LANG=ru_RU.UTF-8
uid=app
enable-threads=True
py-call-uwsgi-fork-hooks=True